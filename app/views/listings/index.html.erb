<h1>Show All Listings</h1>
<section id="page">
  <table class="table-striped">
    <thead>
    <tr>
      <th>ID. </th>
      <th>Listing Name</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
    </thead>

    <tbody>
    <% @listing.each do |listing| %>
      <tr>
        <td><%= listing.id.to_s %></td>
        <td><%= listing.listing_name %></td>
        <td><%= listing.created_at.to_s %></td>
        <td><%= listing.updated_at.to_s %></td>
        <td><a href='/listings/<%= listing.id%>/edit'>Edit</a></td>
        <td> <a href="<%= @delete %>/listings/<%= listing.id %> ">Delete</a></td>
      </tr>
    <% end %>
    </tbody>
  </table>
  <nav>
    <ol>
      <li><a href="/listings">Listings Home</a></li>
      <li><a href="/listings/new">New</a></li>

    </ol>
  </nav>
</section>